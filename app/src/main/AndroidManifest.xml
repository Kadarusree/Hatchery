<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="shiva.com.hatchery">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_aquarium"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_aquarium"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".TankDetals"
            android:label="Tank Details"
            android:parentActivityName=".DashboardActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".LoginActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Signup" />
        <activity
            android:name=".DashboardActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ATU_Activity"
            android:label="@string/opt6"
            android:parentActivityName=".DashboardActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".EOD_Checkilist"
            android:label="End of Day Checklist"
            android:parentActivityName=".DashboardActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".oxygentemp.OxygenTemperature"
            android:label="@string/opt1"
            android:parentActivityName=".DashboardActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".feedingData.DailyFeedingData"
            android:label="@string/opt2"
            android:parentActivityName=".DashboardActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".oxygentemp.OxygenTemperatureResults"
            android:screenOrientation="landscape" />
        <activity android:name=".EOD_CHK_History" android:screenOrientation="landscape" android:parentActivityName=".EOD_Checkilist"></activity>
    </application>

</manifest>